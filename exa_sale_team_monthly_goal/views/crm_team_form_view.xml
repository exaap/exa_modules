<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="crm_team_salesteams_view_form">
    <field name="model">crm.team</field>
    <field name="inherit_id" ref="sale.crm_team_salesteams_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='invoiced_target']" position="after">
        <field name="monthly_goal"/>
      </xpath>
    </field>
  </record>


  <record model="ir.ui.view" id="crm_team_view_form">
    <field name="model">crm.team</field>
    <field name="inherit_id" ref="sales_team.crm_team_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="//page[@name='dashboard']" position="inside">
        <group string="Otros Objetivos">
          <field name="lubrication"/>
          <field name="filtration"/>
          <field name="other_one"/>
          <field name="other_two"/>
          <field name="other_three"/>
        </group>
      </xpath>
    </field>
  </record>

</odoo>
